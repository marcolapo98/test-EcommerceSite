<!DOCTYPE html>
<html>
	<head>
		<meta charset="ISO-8859-1">
		<title>Ralph Lauren - Uomo</title>
		<!-- @include _css.kit -->
	</head>
	<body>
		<div class="container">
			<!-- @include _header.kit -->
			<!-- @include _nav.kit -->
			<form action="demo_form.asp">
				<div class="row">
					<div class="col-sm-4 profile-label">
						<label for="Nome">Nome</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="text" placeholder="Nome" class="form-control" id="Nome" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label for="Cognome">Cognome</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="text" placeholder="Cognome" class="form-control" id="Cognome" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label for="Data di Nascita">Data di Nascita</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="date" placeholder="Data di Nascita" class="form-control" id="Data di Nascita" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label for="E-mail">E-mail</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="email" placeholder="E-mail" class="form-control" id="E-mail" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label for="Password">Password</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="password" placeholder="Password" class="form-control" id="Password" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label for="Conferma Password">Conferma Password</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="password" placeholder="Conferma Password" class="form-control" id="Conferma Password" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label for="Telefono">Telefono</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="tel" placeholder="Telefono" class="form-control" id="Telefono" style="width:200px">
					</div>
				</div>
				
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label>Sesso:</label>
					</div>
					<div class="col-sm-8 profile-input">
						<input type="radio" name="gender" id="male" value="male">
						<label for="male">Uomo</label>
						<input type="radio" name="gender" id="female" value="female">
						<label for="female">Donna</label>
					</div>
				</div>
				<div class="row">
					<div class="col-sm-4 text-right profile-label">
						<label>Preferenze:</label>
					</div>
					<div class="col-sm-8 profile-input">
						<div class="row">
							<input type="checkbox" id="sport">
							<label for="sport">Sport</label>
						</div>
						<div class="row">
							<input type="checkbox" id="cinema">
							<label for="cinema">Cinema</label>
						</div>
						<div class="row">
							<input type="checkbox" id="musica">
							<label for="musica">Musica</label>
						</div>
						<div class="row">
							<input type="checkbox" id="videogame">
							<label for="videogame">Video-Games</label>
						</div>
						<div class="row">
							<input type="submit" value="submit">
						</div>
					</div>
				<div class="row">
					<div class="col-sm-4 profile-label">
					</div>
					<div class="col-sm-8 input-label">
						<input type="checkbox" id="accetto">
						<label for="accetto">Accetto</label>
						la
